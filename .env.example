# Rename this file to .env and fill in your secrets

# === PostgreSQL Database (Shared for local dev or production) ===
# For local docker-compose, the host is the service name: eido_db
# For production (e.g., Fly.io), use the provided connection string.
DATABASE_URL="postgresql+asyncpg://user:password@eido_db:5432/eido_db"

# === EIDO Agent LLM Settings ===
EIDO_LLM_PROVIDER="google"
EIDO_GOOGLE_API_KEY="YOUR_GOOGLE_API_KEY_HERE"
EIDO_GOOGLE_MODEL_NAME="gemini-2.5-flash-lite"
EIDO_OPENAI_API_KEY=""
EIDO_OPENAI_MODEL_NAME="gpt-4o"
EIDO_OPENROUTER_API_KEY=""
EIDO_LOCAL_LLM_URL=""

# === IDX Agent LLM Settings ===
# Note: These are separate from EIDO agent settings.
IDX_LLM_PROVIDER="google"
IDX_GOOGLE_API_KEY="YOUR_GOOGLE_API_KEY_HERE"
IDX_GOOGLE_MODEL_NAME="gemini-1.5-flash-latest"
IDX_OPENAI_API_KEY=""
IDX_OPENAI_MODEL_NAME="gpt-4o"
IDX_LOCAL_LLM_URL=""

# === Geocoding Agent LLM Settings ===
GEOCODING_LLM_PROVIDER="google"
GEOCODING_GOOGLE_API_KEY="YOUR_GOOGLE_API_KEY_HERE"
GEOCODING_GOOGLE_MODEL_NAME="gemini-2.5-flash-lite"

# === Dashboard Settings ===
# A long, random string used for signing authentication tokens.
DASHBOARD_SECRET_KEY="a-super-secret-key-that-should-be-changed"
