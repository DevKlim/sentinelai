# PostgreSQL Database Configuration
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_DB=eido_db

# EIDO Agent Configuration
# LLM_PROVIDER can be 'google' or 'openrouter'
LLM_PROVIDER=google

# Add your API keys here
# Make sure to replace 'your_google_api_key' and 'your_openrouter_api_key' with your actual keys
GOOGLE_API_KEY=your_google_api_key
OPENROUTER_API_KEY=your_openrouter_api_key

# Geocoding service user agent
GEOCODING_USER_AGENT=SDSC-Orchestrator/1.0 (contact: your_email@example.com)
