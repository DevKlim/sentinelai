# Rename this file to .env and fill in your secrets

# Get your key from Google AI Studio: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY="YOUR_GOOGLE_API_KEY_HERE"

# Optional: You can override the default model here
# GOOGLE_MODEL_NAME="gemini-1.5-pro-latest"

# PostgreSQL Database credentials (defaults are fine for local development)
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_DB=eido_db

# Required for the geocoding service to make requests to OpenStreetMap
GEOCODING_USER_AGENT=sdsc-orchestrator-project